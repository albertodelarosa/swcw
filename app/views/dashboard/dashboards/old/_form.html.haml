= form_for @dashboard_dashboard do |f|
  - if @dashboard_dashboard.errors.any?
    #error_explanation
      %h2= "#{pluralize(@dashboard_dashboard.errors.count, "error")} prohibited this dashboard_dashboard from being saved:"
      %ul
        - @dashboard_dashboard.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :index
    = f.text_field :index
  .actions
    = f.submit 'Save'
